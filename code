            <p>Sanctuary of Kira © Year I of the New World - This sacred space is devoted to the divine vision of a purified world</p>
            <p>Devotees: 2,478,916 | Last Revelation: XIII Day of the Blood Moon</p>
            <p>All who oppose divine justice shall be judged</p>
        </footer>
    </div>

    <script>
        // Enhanced counter animation with random fluctuations
        document.addEventListener('DOMContentLoaded', function() {
            const counterElement = document.querySelector('.counter-number');
            let count = 2478916;
            const targetCount = 2478916 + Math.floor(Math.random() * 5000);
            
            const updateCounter = () => {
                // Random growth with occasional decreases to simulate realistic fluctuation
                const change = Math.random() > 0.1 ? Math.floor(Math.random() * 15) + 1 : -Math.floor(Math.random() * 5);
                count += change;
                
                // Ensure we don't go below initial value
                if (count < 2478916) count = 2478916;
                
                counterElement.textContent = count.toLocaleString();
                
                if (count < targetCount) {
                    setTimeout(updateCounter, Math.random() * 1000 + 500);
                }
            };
            
            setTimeout(updateCounter, 1000);
            
            // Add flickering effect to elements with the flicker class
            const flickerElements = document.querySelectorAll('.flicker');
            setInterval(() => {
                flickerElements.forEach(el => {
                    el.style.opacity = (0.7 + Math.random() * 0.3).toString();
                });
            }, 3000);
            
            // Add occasional blood red pulsing effect
            setInterval(() => {
                document.body.style.boxShadow = '0 0 50px rgba(138, 3, 3, 0.5)';
                setTimeout(() => {
                    document.body.style.boxShadow = 'none';
                }, 300);
            }, 10000);
        });
    </script>
</body>
</html>
